{
  "name": "bootstrap-npm-starter",
  "version": "1.4.0",
  "private": true,
  "description": "Demo project for building a Bootstrap powered site via npm.",
  "author": "mdo",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "build": "npm-run-all --parallel css js",
    "css": "npm-run-all css-compile css-prefix css-minify",
    "css-compile": "sass --style expanded --source-map --embed-sources --no-error-css scss/starter.scss:assets/css/oec.css",
    "css-prefix": "npm-run-all --parallel css-prefix-*",
    "css-prefix-main": "postcss --config postcss.config.js --replace \"assets/css/*.css\" \"!assets/css/*.min.css\"",
    "css-lint": "stylelint scss/",
    "css-minify": "npm-run-all --parallel css-minify-*",
    "css-minify-main": "cleancss -O1 --format breakWith=lf --source-map --source-map-inline-sources --output assets/css/oec.min.css assets/css/oec.css",
    "js": "npm-run-all js-compile js-minify",
    "js-compile": "npm-run-all --parallel js-compile-*",
    "js-compile-standalone": "rollup --environment BUNDLE:false --config rollup.config.js --sourcemap",
    "js-compile-standalone-esm": "rollup --environment ESM:true,BUNDLE:false --config rollup.config.js --sourcemap",
    "js-compile-bundle": "rollup --environment BUNDLE:true --config rollup.config.js --sourcemap",
    "js-compile-plugins": "node build-plugins.js",
    "js-lint": "eslint --cache --cache-location .cache/.eslintcache --report-unused-disable-directives .",
    "js-minify": "npm-run-all --parallel js-minify-*",
    "js-minify-standalone": "terser --compress passes=2 --mangle --comments \"/^!/\" --source-map \"content=assets/js/oec.js.map,includeSources,url=oec.min.js.map\" --output assets/js/oec.min.js assets/js/oec.js",
    "js-minify-standalone-esm": "terser --compress passes=2 --mangle --comments \"/^!/\" --source-map \"content=assets/js/oec.esm.js.map,includeSources,url=oec.esm.min.js.map\" --output assets/js/oec.esm.min.js assets/js/oec.esm.js",
    "js-minify-bundle": "terser --compress passes=2 --mangle --comments \"/^!/\" --source-map \"content=assets/js/oec.bundle.js.map,includeSources,url=oec.bundle.min.js.map\" --output assets/js/oec.bundle.min.js assets/js/oec.bundle.js",
    "js-test": "npm-run-all --parallel js-test-karma js-test-jquery js-test-integration-*",
    "server": "browser-sync start --proxy localhost:5000 --files \"assets/css/starter.css\"",
    "storybook": "start-storybook -p 5000 -c storybook/.storybook -s assets/",
    "build:clean": "rimraf build",
    "build:storybook": "build-storybook -c storybook/.storybook -o build",
    "build:copy": "ncp assets build",
    "dist": "npm-run-all build:*",
    "start": "npm-run-all --parallel server watch storybook",
    "watch": "nodemon -e twig,scss -x \"npm run css\"",
    "test": "npm run css-lint && npm run css"
  },
  "keywords": [
    "bootstrap",
    "sass",
    "css",
    "javascript",
    "starter",
    "template"
  ],
  "workspaces": [
    "storybook"
  ],
  "dependencies": {
    "bootstrap": "^5.0.0-beta2",
    "bootstrap-icons": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "clean-css-cli": "^4.3.0",
    "@popperjs/core": "^2.6.0",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "autoprefixer": "^10.2.0",
    "browser-sync": "^2.26.14",
    "glob": "^7.1.6",
    "eslint": "^7.19.0",
    "sass": "^1.32.8",
    "ncp": "2.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.5",
    "postcss-cli": "^8.3.1",
    "purgecss": "^3.1.3",
    "rimraf": "3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-istanbul": "^3.0.0",
    "serve": "^11.3.2",
    "stylelint": "^13.9.0",
    "stylelint-config-twbs-bootstrap": "^2.1.0",
    "terser": "5.1.0"
  }
}
