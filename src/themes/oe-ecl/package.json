{
  "name": "@oe/theme-ecl",
  "author": "European Commission",
  "license": "EUPL-1.1",
  "version": "0.0.1",
  "description": "OE â€” theme ECL",
  "private": true,
  "scripts": {
    "css-clean": "rimraf css",
    "css-compile": "sass --load-path=../../../node_modules --style expanded --source-map --embed-sources --no-error-css oe-ecl.scss:css/oe-ecl.css",
    "css-prefix": "postcss --config postcss.config.js --replace \"css/*.css\"",
    "css-minify": "cleancss -O1 --format breakWith=lf --source-map --source-map-inline-sources --output css/oe-ecl.min.css css/oe-ecl.css",
    "js-clean": "rimraf js",
    "js-compile": "npm-run-all js-compile-*",
    "js-compile-standalone": "rollup --environment BUNDLE:false --config scripts/rollup.config.js --sourcemap",
    "js-compile-standalone-esm": "rollup --environment ESM:true,BUNDLE:false --config scripts/rollup.config.js --sourcemap",
    "js-compile-bundle": "rollup --environment BUNDLE:true --config scripts/rollup.config.js --sourcemap",
    "js-lint": "eslint --cache --cache-location .cache/.eslintcache --report-unused-disable-directives .",
    "js-minify": "npm-run-all js-minify-*",
    "js-minify-standalone": "terser --compress passes=2 --mangle --comments \"/^!/\" --source-map \"content=js/oe-ecl.js.map,includeSources,url=oe-ecl.min.js.map\" --output js/oe-ecl.min.js js/oe-ecl.js",
    "js-minify-standalone-esm": "terser --compress passes=2 --mangle --comments \"/^!/\" --source-map \"content=js/oe-ecl.esm.js.map,includeSources,url=oe-ecl.esm.min.js.map\" --output js/oe-ecl.esm.min.js js/oe-ecl.esm.js",
    "js-minify-bundle": "terser --compress passes=2 --mangle --comments \"/^!/\" --source-map \"content=js/oe-ecl.bundle.js.map,includeSources,url=oe-ecl.bundle.min.js.map\" --output js/oe-ecl.bundle.min.js js/oe-ecl.bundle.js",
    "twig": "copyfiles -f ../../components/**/*.twig templates/"
  },
  "devDependencies": {
    "@oe/bootstrap": "0.0.1",
    "@oe/theme-oe": "0.0.1",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-replace": "2.4.2",
    "clean-css-cli": "5.3.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.48.0",
    "rollup-plugin-istanbul": "3.0.0",
    "sass": "1.32.13",
    "terser": "5.7.0"
  }
}
